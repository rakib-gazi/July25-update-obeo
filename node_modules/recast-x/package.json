{
  "name": "recast-x",
  "author": "Pionxzh",
  "version": "1.0.5",
  "packageManager": "pnpm@8.15.7",
  "description": "JavaScript syntax tree transformer, nondestructive pretty-printer, and automatic source map generator",
  "keywords": [
    "ast",
    "rewriting",
    "refactoring",
    "codegen",
    "syntax",
    "transformation",
    "parsing",
    "pretty-printing"
  ],
  "homepage": "http://github.com/pionxzh/recast-x",
  "repository": {
    "type": "git",
    "url": "git://github.com/pionxzh/recast-x.git"
  },
  "license": "MIT",
  "main": "main.js",
  "types": "main.d.ts",
  "lint-staged": {
    "*.ts": [
      "eslint",
      "prettier -c"
    ]
  },
  "browser": {
    "fs": false
  },
  "dependencies": {
    "ast-types": "npm:ast-types-x@1.18.0",
    "source-map": "~0.6.1",
    "tiny-invariant": "^1.3.3",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.24.9",
    "@babel/parser": "7.24.8",
    "@babel/preset-env": "7.24.8",
    "@types/esprima": "4.0.6",
    "@types/estree": "^1.0.5",
    "@types/glob": "8.1.0",
    "@types/mocha": "10.0.7",
    "@types/node": "20.14.12",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "esprima": "~4.0.0",
    "esprima-fb": "15001.1001.0-dev-harmony-fb",
    "flow-parser": "0.195.0",
    "glob": "8.0.3",
    "lint-staged": "^15.2.7",
    "mocha": "^10.7.0",
    "prettier": "^2.6.2",
    "reify": "0.20.12",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
    "mocha": "test/run.sh",
    "debug": "test/run.sh --inspect-brk",
    "test": "npm run lint && npm run build && npm run mocha",
    "run-test": "mocha --reporter spec --full-trace test/run.js",
    "build": "npm run clean && tsc",
    "lint": "eslint --ext .ts .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "tsc --build --clean"
  }
}